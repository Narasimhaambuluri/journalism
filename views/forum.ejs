<div class="container py-5 bg-light mt-2 mb-5 rounded">
    <h4 class="mx-4">Community Forum</h4>
    <div class="row p-3">
        <% posts.forEach(entry=> { %>
            <div class="col-md-12 m-2">
                <div class="col-md-">
                    <div class="card">
                        <div class="card-body">
                            <%= entry.entry %>
                        </div>
                    </div>
                </div>
            </div>
         <% }); %>
    </div>
</div>